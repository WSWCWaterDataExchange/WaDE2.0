{
	"name": "Utah Normalization",
	"properties": {
		"activities": [
			{
				"name": "Excel to Json",
				"type": "AzureFunctionActivity",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"functionName": {
						"value": "@concat('ExcelConversionToJson?container=utahwaterrights&fileName=UT_Master_Water_Rights_Exchange.csv&folder= Test_Normalization')",
						"type": "Expression"
					},
					"method": "GET"
				},
				"linkedServiceName": {
					"referenceName": "WaDE Import Functions",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "UT Water Sources",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Excel to Json",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSetting"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSetting",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator"
					}
				},
				"inputs": [
					{
						"referenceName": "UT_Master_Water_Rights_Exchange_json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "UT_NormalizedWaterAllocations",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "State Normalization"
		}
	}
}