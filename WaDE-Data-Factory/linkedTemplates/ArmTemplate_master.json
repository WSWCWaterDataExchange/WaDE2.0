		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"Blob_CO_Amounts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob_CO_Amounts'"
		},
		"WaDE Database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'WaDE Database'"
		},
		"WaDE Import Functions_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'WaDE Import Functions'"
		},
		"ColoradoOrganizationsFile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(dataset().FolderIdentifier, '/Organizations.json')"
